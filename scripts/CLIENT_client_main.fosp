		y=draw_timeouts(critter,(233),"Science",x,y);
		if(i==4&&critter.Timeout[(232)]>0)
		y=draw_timeouts(critter,(232),"Repair",x,y);
		if(i==5&&critter.Timeout[(231)]>0)
