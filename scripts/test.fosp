	uint keyId=Random(10000,50000);
	car.LockerId=keyId;
	car.LockerComplexity=1000;
	car.GetChild(0).LockerId=keyId;
	car.GetChild(0).LockerComplexity=1000;
	cr.AddItem((97),1).LockerId=keyId;
	
	car.Update();
	cr.Say((11),"Done.");
}

