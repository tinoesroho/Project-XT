	int curPos=((cr.Anim1Life&(0x0100))!=0?1:((cr.Anim1Life&(0x0200))!=0?2:0));
	switch(curPos)
	{
		case 0:
		switch(newPos)
		{
			case 1:
			cr.Animate(0,(11),null,true,true);
			break;
			case 2:
			cr.Animate(0,(11),null,true,true);
			cr.Animate((0x0100),(12),null,false,true);
			break;
			default:
			break;
		}
		break;
		case 1:
		switch(newPos)
		{
			case 0:
			cr.Animate(0,(10),null,true,true);
			break;
			case 2:
			cr.Animate(0,(12),null,true,true);
			break;
