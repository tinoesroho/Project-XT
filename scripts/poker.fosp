{
	return(digit>>(bnum-1))&1;
}

uint64 SetBit(uint64 digit,uint8 bnum,bool value)
{
	if(value)
	digit|=(1<<(bnum-1));
	else
	digit&=~(1<<(bnum-1));
	return digit;
}

uint8 SetBit5(uint8 digit,uint8 bnum,bool value)
{
	if(value)
	digit|=(1<<(bnum-1));
	else
	digit&=~(1<<(bnum-1));
	return digit;
}

uint8 GetSuit(uint8 CNum)
{   
	
	if(CNum<=13)
	{
		return(0);
	}
	else
	{
		if(CNum<=26)
		{
			return(1);
		}
		else
		{
			if(CNum<=39)
			{
				return(2);
			}
			else
			{
				return(3);
			}
		}
	}
}

uint8 GetRank(uint8 CNum)
{    
	
	return(CNum-1)%13;
}

