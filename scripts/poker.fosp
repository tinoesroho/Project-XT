	for(uint8 i=0;i<((4)*(5));i++)
	{
		GameCards[pokerMan.StatBase[(99)]][i]=0;
	}
	Bank[pokerMan.StatBase[(99)]]=(4)*MinBet[pokerMan.StatBase[(99)]];
