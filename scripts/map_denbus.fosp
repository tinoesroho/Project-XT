                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 

void _DenBusInit(Map&denBus,bool firstTime)
{
	denBus.SetEvent((6),"_DenBusInCritter");
	denBus.SetEvent((7),"_DenBusOutCritter");
}

void _DenBusInCritter(Map&denBus,Critter&player)
{
	if(player.IsPlayer())
	player.SayMsg((11),(0),(1050));
}

void _DenBusOutCritter(Map&denBus,Critter&player)
{
	if(player.IsPlayer())
	player.SayMsg((11),(0),(1051));
}
