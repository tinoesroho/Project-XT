void _MapInit(Map&map,bool firstTime)
{
	_DeadlyRadiation(map,firstTime);
	Critter@[]critters;
	map.GetCritters(0,(0x01)|(0x20),critters);
	
	for(uint i=0;i<critters.length();i++)
	{
		critters[i].ToDead((116),null);
		critters[i].StatBase[(84)]=-1;
	}
}
