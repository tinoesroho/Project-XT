{
	Critter@[]critters;
	map.GetCritters(0,(0x01)|(0x20),critters);
	for(uint i=0;i<critters.length();i++)
	{
		critters[i].StatBase[(84)]=-1;
		critters[i].ModeBase[(532)]=1;
	}
}

void _VBoyInit(Critter&npc,bool firstTime)
{
	npc.StatBase[(67)]=(249);
}
